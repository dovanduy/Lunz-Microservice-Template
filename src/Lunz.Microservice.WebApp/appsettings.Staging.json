      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Information"
      }
    }
  },
  "Apollo": {
    "AppId": "OrderManagementTemplate",
    "MetaServer": "http://10.24.41.187:8180",
